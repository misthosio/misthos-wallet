{
  "name": "misthos",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@jsiebern/bs-material-ui": "0.3.1",
    "@material-ui/core": "1.2.3",
    "autosuggest-highlight": "3.1.1",
    "bignumber.js": "7.0.1",
    "bip39": "2.5.0",
    "bitcoin-ops": "1.4.1",
    "bitcoinjs-lib": "3.3.2",
    "blockstack": "17.2.0",
    "bs-css": "7.0.1",
    "bs-fetch": "0.3.0",
    "bs-json": "1.0.1",
    "bs-webapi": "0.10.0",
    "clipboard": "2.0.1",
    "json-stable-stringify": "1.0.1",
    "material-ui": "1.0.0-beta.47",
    "react": "16.4.1",
    "react-autosuggest": "9.3.4",
    "react-dom": "16.4.1",
    "reason-reroute": "0.0.4",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "0.4.2",
    "babel-core": "6.26.3",
    "babel-jest": "23.2.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.7.0",
    "bs-platform": "3.1.5",
    "css-loader": "0.28.11",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.1",
    "next": "6.1.1",
    "raf": "3.4.0",
    "reason-react": "0.4.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.15.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "whatwg-fetch": "2.0.4",
    "worker-loader": "2.0.0",
    "xmlhttprequest": "1.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{re,ml}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*_test.bs.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|re|ml)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|re|ml)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "re",
      "ml",
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "env"
        ]
      },
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      }
    }
  }
}
