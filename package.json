{
  "name": "misthos",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@glennsl/bs-json": "5.0.2",
    "@jsiebern/bs-material-ui": "0.4.3-beta.2",
    "@ledgerhq/hw-app-btc": "4.24.0",
    "@ledgerhq/hw-transport-u2f": "4.24.0",
    "@material-ui/core": "3.5.1",
    "autosuggest-highlight": "3.1.1",
    "bignumber.js": "8.0.2",
    "bitcoin-ops": "1.4.1",
    "bitcoinjs-lib": "4.0.5",
    "blockstack": "19.2.1",
    "bs-css": "8.0.0",
    "bs-fetch": "0.3.1",
    "bs-webapi": "0.13.4",
    "clipboard": "2.0.6",
    "formdata-polyfill": "3.0.19",
    "json-stable-stringify": "1.0.1",
    "raven-js": "3.27.2",
    "react": "16.6.3",
    "react-autosuggest": "9.4.3",
    "react-dom": "16.6.3",
    "react-jss": "8.6.1",
    "reason-reroute": "callstackincubator/reroute#15/head",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "0.5.1",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-preset-env": "1.7.0",
    "bs-platform": "5.0.6",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.5",
    "next": "9.3.2",
    "raf": "3.4.1",
    "reason-react": "0.5.3",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "whatwg-fetch": "2.0.4",
    "worker-loader": "2.0.0",
    "xmlhttprequest": "1.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{re,ml}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*_test.bs.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|re|ml)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|re|ml)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "re",
      "ml",
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "env"
        ]
      },
      "development": {
        "presets": [
          "next/babel"
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ]
      }
    }
  }
}
